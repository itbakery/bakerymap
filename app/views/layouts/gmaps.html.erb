          </ul>
        </div><!--/.nav-collapse -->
      </div>
    </div>
<div id='map' style="margin-top: 50px;" class="cbp-spmenu-push"></div>



<script type="text/javascript" language="javascript" charset="utf-8">
//<![CDATA[

  var menuRight = document.getElementById( 'cbp-spmenu-s1' ),
      menuLeft = document.getElementById( 'cbp-spmenu-s2' ),
      gmap = document.getElementById('map'),
      body = document.body;

  showRight.onclick = function() {
    classie.toggle( this, 'active' );
    classie.toggle( menuRight, 'cbp-spmenu-open' );
  };
  showLeftPush.onclick = function() {
      classie.toggle( this, 'active' );
      classie.toggle( body, 'cbp-spmenu-push-toright' );
      classie.toggle( gmap, 'cbp-map-spmenu-push-toright' );
      classie.toggle( menuLeft, 'cbp-spmenu-open' );
  };


var map;
function initialize() {
  var mapOptions = {
    center: new google.maps.LatLng(14.3180935416, 101.872305154),
    zoom: 13
  };
  map = new google.maps.Map(document.getElementById('map'),mapOptions);


  var thaisamakkee = new google.maps.visualization.MapsEngineLayer({
    layerId: '01396710784617750777-09507073323105492707',
    map: map,
    clickable: true
  });
  console.log(thaisamakkee);
}

google.maps.event.addDomListener(window, 'load', initialize);


//]]>
</script>
